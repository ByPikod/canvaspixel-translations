#: src\utils\validation.js:17
msgid "Email can't be empty."
msgstr "Необхідно вказати адресу електронної скриньки."

#: src\utils\validation.js:18
msgid "Email should be at least 5 characters long."
msgstr "Електронна скринька не може бути коротша за 5 символів."

#: src\utils\validation.js:19
msgid "Email can't be longer than 40 characters."
msgstr "Електронна скринька не може бути довша за 40 символів."

#: src\utils\validation.js:20
msgid "Email should at least contain a dot"
msgstr "Електронна скринька має містити в собі мінімум одну крапку"

#: src\utils\validation.js:22
msgid "Email should contain a @"
msgstr "Електронна скринька має містити в собі символ @"

#: src\utils\validation.js:29
msgid "Name can't be empty."
msgstr "Ім'я не може бути порожнім."

#: src\utils\validation.js:30
msgid "Name must be at least 2 characters long"
msgstr "Ім'я не може бути коротшим ніж 2 символи"

#: src\utils\validation.js:31
msgid "Name must be shorter than 26 characters"
msgstr "Ім'я має бути коротшим за 26 символів"

#: src\utils\validation.js:38
msgid "Name contains invalid character like @, /, \\ or #"
msgstr "Ім'я містить в собі неприпустимі символи, такі як @, /, \\ чи #"

#: src\utils\validation.js:53
msgid "No password given."
msgstr "Пароль не вказано."

#: src\utils\validation.js:56
msgid "Password must be at least 6 characters long."
msgstr "Пароль не може бути коротшим за 6 символів."

#: src\utils\validation.js:59
msgid "Password must be shorter than 60 characters."
msgstr "Пароль має бути коротшим за 60 символів."

